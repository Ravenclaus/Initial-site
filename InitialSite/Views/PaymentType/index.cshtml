@model IEnumerable<InitialSite.Models.PaymentType>

@{
    ViewBag.Title = "index";
}

<h2>index</h2>

<p>
    @Html.ActionLink("Create New", "AddPaymentType")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PaymentTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PaymentTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccountNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PaymentTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PaymentTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PaymentTypeId })
        </td>
    </tr>
}

</table>
